# === REQUIRED BOT CONFIGURATION ===
# Your bot's secret token from the Discord Developer Portal
DISCORD_BOT_TOKEN=your_discord_token

# MongoDB connection string (e.g., mongodb+srv://...)
MONGO_URI=your_mongodb_uri

# Google Gemini API key for AI chat and image generation
GEMINI_API_KEY=your_gemini_api_key

# Your Discord User ID (so you have full control over the bot)
BOT_OWNER_ID=your_bot_owner_id

# === SUPPORT & BRANDING ===
# Email displayed in the /contact command
CONTACT_EMAIL=your_contact_email

# Invite link for your support server
SUPPORT_SERVER=your_support_server_link

# The ID of your support server
SUPPORT_SERVER_ID=your_support_server_id

# === LOGGING & SYSTEM ===
# The ID of the category where global bot logs will be created
GLOBAL_LOG_CATEGORY_ID=your_log_category_id

# A random string used for session encryption
SESSION_SECRET=your_session_secret

# === OPTIONAL INTEGRATIONS ===
# Required if you use YouTube notification features
YOUTUBE_API_KEY=your_youtube_api_key

# === SYSTEM GENERATED (Usually safe to leave blank or ignore) ===
REPLIT_DOMAINS=
REPL_ID=
DATABASE_URL=
PGDATABASE=
PGHOST=
PGPORT=
PGUSER=
PGPASSWORD=
